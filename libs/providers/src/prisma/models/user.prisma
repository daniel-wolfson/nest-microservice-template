// Example model from current project
model User {
  id        String        @id @default(uuid())
  name      String
  email     String        @unique
  password  String
  settings  UserSettings?
  createdAt DateTime      @default(now()) @db.Timestamptz(3)
  updatedAt DateTime      @updatedAt @db.Timestamptz(3)
  roles     UserRole[]
}
